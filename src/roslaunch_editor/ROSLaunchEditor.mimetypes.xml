<?xml version="1.0"?>

<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/ros.roslaunch">
    <comment>ROS Launch file types</comment>
    <glob pattern="*.launch"/>
    <magic priority="50">
       <match value="&lt;launch" type="string" offset="0:2000"/>
    </magic>
  </mime-type>
</mime-info>
